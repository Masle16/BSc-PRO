	\node[circle, draw=black,fill=white, inner sep=1pt, minimum size=4pt, name=relu, align=center, label=relu] at (-4.5,8) {\footnotesize$0: x<0$\\ \footnotesize else $x$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=m2] at (-2,8) {$*$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=p2] at (0,8) {$+$};

