	\draw[->,very thick] node[left, above]{} (p1) -- (relu);
	\draw[->,very thick] node[left, above]{} (relu) -- (m2) node[above,midway, text=red]{\footnotesize $sm\cdot W_{2}$};	
	\draw[->,very thick] node[left, above]{} (m2) -- (p2) node[above,midway, text=red]{$sm$};
	\draw[->,very thick] node[left, above]{} (p2) -- (exp);		
