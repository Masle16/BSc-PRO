	\draw[draw=black,fill=white!65!yellow] (3.75,9.6) node[above,label=\textcolor{red}{$\frac{1}{N}\cdot$}\textcolor{blue}{$\frac{e^{y_{i}}}{\sum_{j}e^{y_{j}}}-1=sm$}]{Softmax} (1.4,4.1) rectangle ++(4.7,5.6);
    \node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=exp] at (2,8) {$e^{x}$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=sum] at (3,9) {$\sum{x}$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=div1] at (5,9) {$\frac{1}{x}$};

	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=y] at (3,7) {$y_{i}$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=m3] at (5,7) {$*$};

	\node[circle, draw=black,fill=white, inner sep=1pt, minimum size=4pt, name=log] at (5,5) {\footnotesize$-1\cdot\log{x}$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=m4] at (5,3) {$*$};

	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=div2] at (3,3) {$\frac{1}{x}$};	

	%Regulization
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=s1] at (0,-2.5) {$x^2$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=s2] at (0,-1.5) {$x^2$};
