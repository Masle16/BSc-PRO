	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=s4] at (0,1) {$x^2$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=m4] at (5,3) {$*$};		
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=p3] at (5,-1) {$+$};
	\node[circle, draw=black,fill=white, inner sep=2.5pt, minimum size=4pt, name=m5] at (7,1) {$*$};
	%End
	\node[circle, draw=black,fill=white, inner sep=0pt, minimum size=4pt, name=p4] at (7,3) {$+$};

	%Inputs
	\draw[->,very thick] (1,3) node[left, above]{N} (1,3) -- (div2);
	\draw[->,very thick] (-10,9) node[left, above]{$x_{input}$} (-10,9) --(-9,9)-- (m1);
	\draw[->,very thick] (-10,7)node[above, text=red]{\textcolor{red}{\footnotesize $sm\cdot W_{2}\cdot1(x>=0)\cdot x_{input}$}} node[left]{$W_{1}$} (-10,7) --(-8,7) -- (m1);
	\draw[->,very thick] (-10,6) node[left]{$b_{1}$} (-10,6) --(-7,6) node[above,midway, text=red]{\textcolor{red}{\footnotesize $sm\cdot W_{2}\cdot$}\footnotesize \textcolor{blue}{$1(x>=0)$}}-- (p1);
	\draw[->,very thick] (-10,5) node[left]{$W_{2}$} (-10,5) --(-3,5) node[above,midway, text=red]{$sm\cdot relu$}-- (m2)  ;
	\draw[->,very thick] (-10,4) node[left]{$b_{2}$} (-10,4) --(-1,4) node[above,midway, text=red]{$sm$}-- (p2) ;
	\draw[->,very thick] (-10,3) node[left]{$\lambda$} (-10,3) --(-2,3) node[above,midway, text=red]{$W_{1}^2+b_{1}^2+W_{2}^2+b_{2}^2$}-- (m5);

	\draw[->,very thick] (-10,7) node[left, above]{} (-11,7) --(-12.5,7)--(-12.5,-2.5) -- (s1) node[above,midway]{\textcolor{red}{$\lambda\cdot$}\textcolor{blue}{$2\cdot W_{1}$}};
	\draw[->,very thick] (-10,6) node[left, above]{} (-11,6) --(-12,6)--(-12,-1.5)-- (s2) node[above,midway]{\textcolor{red}{$\lambda\cdot$}\textcolor{blue}{$2\cdot b_{1}$}};
	\draw[->,very thick] (-10,5) node[left, above]{} (-11,5) --(-11.5,5)--(-11.5,0)-- (s3) node[above,midway]{\textcolor{red}{$\lambda\cdot$}\textcolor{blue}{$2\cdot W_{2}$}};
	\draw[->,very thick] (-10,4) node[left, above]{} (-10.7,4) --(-11,4)--(-11,1)-- (s4) node[above,midway]{\textcolor{red}{$\lambda\cdot$}\textcolor{blue}{$2\cdot b_{2}$}};

	%Output
	\draw[->,very thick] (9,3)  node[left, above]{loss} (p4) -- (9,3) node[above,midway, text=red]{$\frac{\partial loss}{\partial loss}$};

	%Internal lines
